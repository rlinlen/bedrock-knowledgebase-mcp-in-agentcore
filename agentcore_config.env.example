# AgentCore Configuration Environment Variables
# Copy this file to agentcore_config.env and fill in your actual values

# AWS IAM Role ARN for AgentCore execution (optional, can use auto_create_execution_role=True)
ROLE_ARN=arn:aws:iam::YOUR_ACCOUNT_ID:role/your_execution_role

# Cognito Configuration
COGNITO_CLIENT_ID=your_cognito_client_id_here
COGNITO_DISCOVERY_URL=https://cognito-idp.REGION.amazonaws.com/USER_POOL_ID/.well-known/openid-configuration

# AgentCore Tool Configuration
TOOL_NAME=bedrock_knowledgebase_agentcore_mcp

# VPC Configuration (if needed)
VPC_SUBNETS=subnet-xxxxxxxxx,subnet-yyyyyyyyy
VPC_SECURITY_GROUPS=sg-xxxxxxxxx

# Note: MCP Server environment variables should be configured in .env file
# The deployment script will automatically load both agentcore_config.env and .env